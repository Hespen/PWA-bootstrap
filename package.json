{
  "name": "pwa-bootstrap",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "keywords": [
    "pwa",
    "react",
    "graphql",
    "universal",
    "server side rendering",
    "service worker",
    "boilerplate",
    "jest"
  ],
  "main": "server/index.js",
  "scripts": {
    "start": "npm run local",
    "stop": "pm2 delete pm2.json && rimraf npm-debug.log* babel_cache",
    "prelocal": "mkdir -p ./build/server/ && touch ./build/server/index.js",
    "local": "PWA_ENV=local PWA_PUBLIC_PATH=http://localhost:8080/build/client/ PWA_SSR=false NODE_ENV=development PORT=8000 run-p local:*",
    "local:client": "webpack-dev-server --config ./webpack.client.js --hot",
    "local:server": "webpack --config ./webpack.server.js --watch",
    "local:serve": "pm2 start pm2.json --only pwa-local",
    "postlocal": "npm stop",
    "development": "run-s development:*",
    "development:build": "PWA_ENV=development PWA_PUBLIC_PATH=/build/client/ NODE_ENV=production run-s build:*",
    "development:start": "PWA_SSR=true NODE_ENV=production BASE_URL=http://localhost:1337 PORT=1337 pm2 start pm2.json --only pwa",
    "staging": "npm install --no-optional && run-s staging:*",
    "staging:build": "PWA_ENV=staging PWA_PUBLIC_PATH=//staging.cdn.com/build/client/ NODE_ENV=production run-s build:*",
    "staging:start": "PWA_SSR=true NODE_ENV=production PORT=1337 pm2 start pm2.json --only pwa",
    "production": "npm install --no-optional && run-s production:*",
    "production:build": "PWA_ENV=production PWA_PUBLIC_PATH=/build/client/ NODE_ENV=production run-s build:*",
    "production:start": "PWA_SSR=true NODE_ENV=production PORT=8000 pm2 start pm2.json --only pwa",
    "build:client": "webpack --config ./webpack.client.js --progress",
    "build:server": "webpack --config ./webpack.server.js --progress",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ignore-path .gitignore .",
    "lint:stylelint": "stylelint ./client/**/*.css",
    "test": "NODE_ENV=test jest",
    "pm2": "pm2"
  },
  "pre-commit": "lint, test",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-react-remove-prop-types": "0.4.8",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "css-loader": "0.28.7",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "0.11.2",
    "jest": "^21.2.1",
    "npm-run-all": "4.1.1",
    "pm2": "^2.7.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-url": "^7.1.2",
    "rimraf": "2.6.2",
    "style-loader": "0.18.2",
    "stylelint": "8.1.1",
    "stylelint-config-standard": "17.0.0",
    "url-loader": "0.5.9",
    "webpack-dev-server": "^2.9.1",
    "workbox-webpack-plugin": "^2.0.3"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.0.0",
    "apollo-client": "^2.0.1",
    "apollo-link-http": "^1.0.0",
    "apollo-local-query": "^0.3.0",
    "assets-webpack-plugin": "3.5.1",
    "babel-polyfill": "6.26.0",
    "body-parser": "^1.18.2",
    "classnames": "2.2.5",
    "clean-webpack-plugin": "^0.1.17",
    "compression": "^1.7.1",
    "connect-slashes": "1.3.1",
    "copy-webpack-plugin": "^4.1.0",
    "express": "^4.16.0",
    "extract-css-chunks-webpack-plugin": "2.0.17",
    "extract-text-webpack-plugin": "^3.0.0",
    "fs": "0.0.1-security",
    "graphql": "^0.11.6",
    "graphql-server-express": "^1.2.0",
    "graphql-tag": "^2.5.0",
    "graphql-tools": "^2.6.1",
    "helmet": "^3.8.2",
    "history": "^4.7.2",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.4",
    "merge-graphql-schemas": "^1.1.4",
    "moment": "2.18.1",
    "morgan": "^1.9.0",
    "nock": "^9.0.22",
    "node-sass": "^4.5.3",
    "normalize.css": "7.0.0",
    "preact": "8.2.5",
    "preact-compat": "3.17.0",
    "prop-types": "^15.6.0",
    "query-string": "5.0.0",
    "raw-loader": "^0.5.1",
    "react": "^16.0.0",
    "react-apollo": "^2.0.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.0.0",
    "react-helmet": "5.2.0",
    "react-router": "^4.2.0",
    "sass-loader": "^6.0.6",
    "sw-precache": "^5.2.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "web-storage": "^1.0.0",
    "webpack": "3.6.0",
    "webpack-bundle-analyzer": "2.9.0",
    "webpack-dashboard": "0.4.0",
    "webpack-node-externals": "1.6.0",
    "workbox-sw": "^2.0.3"
  },
  "optionalDependencies": {
    "pre-commit": "1.2.2"
  },
  "jest": {
    "globals": {
      "__BROWSER__": false,
      "__PWA_ENV__": "local",
      "__LOCAL__": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lakshyaranganath/pwa.git"
  },
  "bugs": {
    "url": "https://github.com/lakshyaranganath/pwa/issues"
  },
  "homepage": "https://github.com/lakshyaranganath/pwa#readme"
}
